/new-home\") {\r\n      setIsNewHome(true);\r\n    }\r\n  }, [isNewHome]);\r\n\r\n  return (\r\n    <>\r\n      {!isNewHome && (\r\n        <div className=\"grid-container\">\r\n          <FancyHeaderComponent device={device} />\r\n          <main>\r\n            {/* <Route exact path=\"/\" render={(props) => <Homepage {...props} />} /> */}\r\n            <Route\r\n              exact\r\n              path=\"/\"\r\n              render={(props) => <SearchPage {...props} />}\r\n            />\r\n            <Route\r\n              exact\r\n              path=\"/details\"\r\n              render={(props) => (\r\n                <DetailPageLoadableComponent {...props} device={device} />\r\n              )}\r\n            />\r\n            <Route\r\n              exact\r\n              path=\"/location-list\"\r\n              render={(props) => <LocationList {...props} device={device} />}\r\n            />\r\n            <Route\r\n              exact\r\n              path=\"/add-new-garage\"\r\n              render={(props) => <AddNewGarage {...props} device={device} />}\r\n            />\r\n          </main>\r\n          <Footer path={window.location.pathname} />\r\n        </div>\r\n      )}\r\n      <Route\r\n        exact\r\n        path=\"/new-home\"\r\n        render={(props) => (\r\n          <LatLongContext.Provider\r\n            value={{\r\n              lat,\r\n              setLat,\r\n              long,\r\n              setLong,\r\n              defaultLocation,\r\n              setDefaultLocation,\r\n            }}\r\n          >\r\n            <RevampHomePage {...props} device={device} />\r\n          </LatLongContext.Provider>\r\n        )}\r\n      />\r\n    </>\r\n  );\r\n};\r\n\r\nexport default withRouter(App);\r\n","D:\\work\\sg-home-page\\UI\\src\\ClearCache.js",[],"D:\\work\\sg-home-page\\UI\\src\\useGaTracker.js",[],"D:\\work\\sg-home-page\\UI\\src\\pages\\searchPage\\searchPage.component.js",[],"D:\\work\\sg-home-page\\UI\\src\\pages\\locationLi